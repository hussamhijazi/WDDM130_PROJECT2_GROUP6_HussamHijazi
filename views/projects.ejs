<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/twstyles.css">
    <link rel="stylesheet" href="/css/main.css">
    <title><%= title %></title>
</head>
<body class="bg-gray-100">

  <%- include('partials/header') %>

  <!-- Projects Section -->
  <section class="projects">
    <div class="projects-header">
        <h1><%= header %></h1>
        <p><%= message %></p>
    </div>
    <div class="projects-list">
        <!-- Project 1 -->
        <a href="/project1" class="project-item">
            <img src="/img/project1/project1-1.png" alt="Project 1" class="project-image">
            <div class="project-content">
                <div class="project-name">Luxury Apartment</div>
                <div class="project-location">Jabal Amman, Jordan</div>
            </div>
        </a>
        <!-- Project 2 -->
        <a href="/project2" class="project-item">
            <img src="/img/project2/project2-1.PNG" alt="Project 2" class="project-image">
            <div class="project-content">
                <div class="project-name">Cantilever Stairs</div>
                <div class="project-location">Dabouq District, Amman</div>
            </div>
        </a>
        <!-- Project 3 -->
        <a href="/project3" class="project-item">
            <img src="/img/project3/project3-1.png" alt="Project 3" class="project-image">
            <div class="project-content">
                <div class="project-name">Luxury Villa</div>
                <div class="project-location">Dabouq District, Amma</div>
            </div>
        </a>
    </div>
  </section>

  <%- include('partials/footer') %>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
  <script src="/js/main.js"></script>
</body>
</html>
